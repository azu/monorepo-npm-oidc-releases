{
  "private": true,
  "name": "monorepo-npm-oidc-releases",
  "version": "1.0.0",
  "description": "monorepo sandbox",
  "keywords": [
    "monorepo"
  ],
  "homepage": "https://github.com/azu/monorepo-npm-oidc-releases",
  "bugs": {
    "url": "https://github.com/azu/monorepo-npm-oidc-releases/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/azu/monorepo-npm-oidc-releases.git"
  },
  "license": "MIT",
  "author": "azu",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "versionup": "lerna version",
    "ci:versionup:patch": "lerna version patch --no-push --no-git-tag-version --yes && npm run commit-version",
    "ci:versionup:minor": "lerna version minor --no-push --no-git-tag-version --yes && npm run commit-version",
    "ci:versionup:major": "lerna version major --no-push --no-git-tag-version --yes && npm run commit-version",
    "commit-version": "git add . && git commit -m \"chore(release): v`node -p 'require(\"./lerna.json\").version'`\"",
    "release": "lerna publish from-package",
    "ci:release": "lerna publish from-package --yes --provenance"
  },
  "devDependencies": {
    "lerna": "^8.2.3"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b"
}
